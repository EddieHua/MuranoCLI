    comp_list=(
      list
      set
      unset
    )
  else
    case $subcmd1 in
      (list)
      more_switches=(
        {--trace}
      )
      ;;
      (set)
      more_switches=(
      )
      ;;
      (unset)
      more_switches=(
      )
      ;;
    esac
