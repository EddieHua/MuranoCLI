  done
  # Add unused subcommand flags.
  for ((idx = 0; idx < ${#more_switches[@]}; idx++)); do
    local switch=${more_switches[$idx]}
    comp_list+=("$switch")
  done

  # If there are only --flags, <TAB> will put the common prefix,
