  # NOTE: Not doing anything special for ${subcmd2}.
  #       Are there sub-sub-commands we shoud list?
  if [[ -z ${subcmd1} || ${len} -eq 1 ]]; then
    comp_list=(
      list
      set
      unset
    )
  else
    case $subcmd1 in
      (list)
      more_switches=(
        {--trace}
      )
      ;;
      (set)
      more_switches=(
      )
      ;;
      (unset)
      more_switches=(
      )
      ;;
    esac
  fi

  # Add unused application-wide flags.
  local idx
