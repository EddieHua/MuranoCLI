        flags_only=false
        break
      fi
    done
    if ${flags_only}; then
      # Use two Unicode en spaces to prevent compgen from prefixing "--".
      comp_list+=("	 ")
    fi
  fi

  COMPREPLY=($(compgen -W '${comp_list[@]}' -- "${last}"))
